<body>
    <section class="about-us about-us-mobile">
        <div class="signin-content-ipad">
            <p>Experience the all - <br>new Finwave Workspace</p>
        </div>
        <div class="container">
            <div class="row">
                <div class="signin-content-mobile">
                    <p>Experience the all - <br>new Finwave Workspace</p>
                </div>
                <div class="col-lg-4 col-md-8 col-sm-12 card workspace-form-signin">
                    <div class="p-4 pnew">
                        <div class="auth-logo"><img src="~/Content/Login/dist-assets/images/Workspace-Logo.svg" alt=""></div>
                      
                        @RenderBody()
                    </div>
                </div>
                <div class="col-lg-7 col-md-7 col-sm-12 mobile-none">
                    <div class="signin-content">
                        <h3>
                            Be A Smart Investor &<br>
                            Ride The Growth Tide With Finwave
                        </h3>
                        <p>Experience the all - new Finwave Workspace</p>
                    </div>
                    <figure class="about-img">
                        <img src="~/Content/Login/dist-assets/images/desk.png" alt="about titan builders photo">
                    </figure>
                </div>
            </div>
        </div>
        <div class="copy-right">
            <p>© 2010 AIRAN FINSTOCKS PVT. LTD. All rights reserved</p>
        </div>
        <div class="thinkpad-copy-right d-flex-01 justify-content-center">
            <p>© 2010 AIRAN FINSTOCKS PVT. LTD. All rights reserved</p>
        </div>
    </section>
</body>
